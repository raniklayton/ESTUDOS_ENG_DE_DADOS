create database ESTUDOS_ENG_DADOS

GO

USE ESTUDOS_ENG_DADOS;


CREATE TABLE CIDADE(
ID_CIDADE INT NOT NULL IDENTITY(1,1),
NOME_CIDADE VARCHAR(100),
ESTADO VARCHAR(100),
PRIMARY KEY(ID_CIDADE)
);

CREATE TABLE TB_DADOS_PESSOAS(
ID_PESSOA INT NOT NULL IDENTITY(1,1),
NOME VARCHAR(250),
ID_CIDADE INT FOREIGN KEY REFERENCES CIDADE(ID_CIDADE),
CPF VARCHAR(11),
RG VARCHAR(20),
PRIMARY KEY(ID_PESSOA)
);

